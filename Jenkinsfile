pipeline{
  agent any
  stages{
    stage("build"){
      steps{
        echo "building the application";
        echo "build,test and deloyment is now lauched automaticaly";
      }
    }
    stage("test"){
      steps{
        echo "testing the application";
        echo "build,test and deloyment is now lauched automaticaly";

      }
    }
    stage("deploy"){
      steps{
        echo "deploying the application";
        echo "build,test and deloyment is now lauched automaticaly";

      }
    }
  }
}
